#include "main.h"

/**
 * print_line - draws a straight line in the terminal
 * @n: lines to print.
 * Return: Always 0
 */

void print_line(int n)
{
  
/**
 * print_line - Prints line of length n
 * @n: Length of line
 */
void print_line(int n)
{
	int in;

	for (in = 0; in < n; in++)
		_putchar('_');

	_putchar('\n');
}
}
